{% extends 'frontOffice/base.html.twig' %}
{% block title %}Qui sommes nous - {{ parent() }}{% endblock %}
{% trans_default_domain 'FOSUserBundle' %}

{% block body %}
    <!-- Sub banner start -->
    <div class="sub-banner">
        <div class="overlay">
            <div class="container">
                <div class="breadcrumb-area">
                    <div class="top">
                        <h1 id="page-title">
                            Qui sommes nous
                        </h1>
                    </div>
                    <ul class="breadcrumbs">
                        <li><a href="{{ path('front_office_homepage') }}">Accueil</a></li>
                        <li class="active">Qui sommes nous</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div class="blog-body">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <!-- Blog box start -->
                    <div class="thumbnail blog-box clearfix">
                        <!-- detail -->
                        <div class="caption detail">
                            <!-- Title -->
                            <h1 class="title">
                                <a href="#">Présentation</a>
                            </h1>
                            <p>
                                Immobiliere.tn est un service fourni par Immobilière 3M sur lequel vous pouvez poster
                                des annonces immobilère gratuitement en acceptant
                                <a href="{{ path('front_office_terms_conditions') }}" class="text-base-color">les
                                    conditions générales d'utilisation</a>
                            </p>
                        </div>
                    </div>
                    <!-- Blog box end-->
                </div>
            </div>
        </div>
    </div>

{% endblock %}
