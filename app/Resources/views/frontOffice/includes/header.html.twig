<!-- Top header start -->
<header class="top-header" id="top">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                <div class="list-inline">
                    <a href="#"><i class="fa fa-phone"></i>+216 31 553 901 / <i class="fa fa-mobile"></i>+216 95 631 100</a>
                    <a href="mailto:contact@immobiliere.tn"><i class="fa fa-envelope"></i>contact@immobiliere.tn</a>
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                <ul class="top-social-media pull-right">
                    {#                    <li class="login">
                                            <a href="#"><i class="fa fa-user"></i></a>
                                        </li>
                                        <li>
                                            <a href="#"><i class="fa fa-envelope-o"></i></a>
                                        </li>
                                        <li>
                                            <a href="#"><i class="fa fa-facebook"></i></a>
                                        </li>
                                        <li>
                                            <a href="#"><i class="fa fa-twitter"></i></a>
                                        </li>
                                        <li>
                                            <a href="#"><i class="fa fa-google-plus"></i></a>
                                        </li>#}
                    {% if is_granted('IS_AUTHENTICATED_REMEMBERED') %}
                        {% if is_granted('ROLE_ADMIN') %}
                            <li>
                                <a href="{{ path('back_office_homepage') }}" class="sign-in">
                                    <i class="fa fa-dashboard"></i>
                                    Administration
                                </a>
                            </li>
                        {% endif %}
                        <li>
                            <a href="{{ path('fos_user_profile_show') }}" class="sign-in"><i class="fa fa-user"></i>
                                {{ app.user.uniqueName }}</a>
                        </li>
                        <li>
                            <a href="{{ path('fos_user_security_logout') }}" class="sign-in"><i
                                        class="fa fa-power-off"></i>
                                Déconnexion</a>
                        </li>
                    {% else %}
                        <li>
                            <a href="#" data-toggle="modal" data-target="#modalLogin" class="sign-in"><i
                                        class="fa fa-key"></i> Se connecter</a>
                        </li>
                        <li>
                            <a href="#" data-toggle="modal" data-target="#modalRegister" class="sign-in"><i
                                        class="fa fa-user"></i> S'inscrire</a>
                        </li>
                    {% endif %}
                </ul>
            </div>
        </div>
    </div>
</header>
<!-- Top header end -->

<!-- Main header start -->
<header class="main-header">
    <div class="container">
        <nav class="navbar navbar-default">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
                        data-target="#app-navigation" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="{{ path('front_office_homepage') }}" class="logo" style="padding: 7px 0px">
                    {#<img src="{{ asset('images/logoo2.png') }}" alt="logo">#}
                    <i class="flaticon-internet" style="font-size: 35px; color: #1abc9c"></i>
                    <span style="color: black; font-size:25px; padding-left: 4px; font-family: Serif">
                        Immobiliere.tn</span>
                </a>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="navbar-collapse collapse" role="navigation" aria-expanded="true" id="app-navigation">
                <ul class="nav navbar-nav">
                    <li class="dropdown">
                        <a tabindex="0" aria-expanded="false" href="{{ path('front_office_homepage') }}">
                            Accueil
                        </a>
                    </li>
                    <li class="dropdown">
                        <a tabindex="0" data-toggle="dropdown" data-submenu="" aria-expanded="false">
                            A vendre<span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            {% for cat in categories %}
                                <li>
                                    <a href="{{ path('front_list_for_sale_properties', {'category_slug': cat.slug}) }}"
                                       tabindex="0">{{ cat.name|capitalize }}</a>
                                </li>
                            {% endfor %}
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a tabindex="0" data-toggle="dropdown" data-submenu="" aria-expanded="false">
                            A louer<span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            {% for cat in categories %}
                                <li>
                                    <a href="{{ path('front_list_for_rent_properties', {'category_slug': cat.slug}) }}"
                                       tabindex="0">{{ cat.name|capitalize }}</a>
                                </li>
                            {% endfor %}
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a tabindex="0" aria-expanded="false" href="{{ path('front_office_contact') }}">
                            Contactez-nous
                        </a>
                    </li>
                    <li class="dropdown active hidden-lg hidden-md">
                        <a class="" tabindex="0" aria-expanded="false" href="{{ path('front_my_properties_new') }}">
                            Déposer une annonce
                        </a>
                    </li>
                </ul>
                <ul class="nav navbar-nav navbar-right rightside-navbar">
                    <li>
                        <a href="{{ path('front_my_properties_new') }}" class="button">
                            Déposer une annonce
                        </a>
                    </li>
                </ul>
            </div>

            <!-- /.navbar-collapse -->
            <!-- /.container -->
        </nav>
    </div>
</header>
<!-- Main header end -->